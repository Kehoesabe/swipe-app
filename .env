# Environment Configuration for Swipe App
# This is your .env file - ready to use!

# Environment
NODE_ENV=development
EXPO_PUBLIC_ENVIRONMENT=development

# App Configuration
EXPO_PUBLIC_APP_NAME=Swipe App
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_SUPPORT_EMAIL=support@swipeapp.com

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# API Configuration (add your API URL when you have one)
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_API_VERSION=v1
EXPO_PUBLIC_API_TIMEOUT=30000

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Development Helpers
EXPO_PUBLIC_MOCK_API=false
EXPO_PUBLIC_OFFLINE_MODE=false
EXPO_PUBLIC_SKIP_AUTH=false

# When you get real services, add them here:
# EXPO_PUBLIC_AUTH_URL=
# EXPO_PUBLIC_CLIENT_ID=
# EXPO_PUBLIC_ANALYTICS_ID=
# EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=
# EXPO_PUBLIC_SENTRY_DSN=