# Swipe Type MVP - Environment Variables Template
# Copy this file to .env and fill in your actual values

# ============================================
# SUPABASE CONFIGURATION
# ============================================
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# ============================================
# STRIPE CONFIGURATION
# ============================================
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# ============================================
# APPLICATION CONFIGURATION
# ============================================
NODE_ENV=development
APP_NAME=Swipe Type
APP_VERSION=1.0.0
SUPPORT_EMAIL=support@swipetype.com

# ============================================
# FEATURE FLAGS
# ============================================
ENABLE_ANALYTICS=true
ENABLE_DEBUG_LOGGING=true
ENABLE_TEST_MODE=true
ENABLE_PREMIUM_REPORTS=true

# ============================================
# SCORING CONFIGURATION
# ============================================
SCORING_ALGORITHM_VERSION=v1.0
MIN_ITEMS_PER_DOMAIN=6
QUALITY_FLAG_THRESHOLDS={"low_variance": 0.3, "speeding": 2.0, "skips": 0.1}

# ============================================
# ANALYTICS (Optional)
# ============================================
GOOGLE_ANALYTICS_ID=your-ga-id
MIXPANEL_TOKEN=your-mixpanel-token
SENTRY_DSN=your-sentry-dsn

# ============================================
# DEVELOPMENT
# ============================================
DEBUG_MODE=true
LOG_LEVEL=debug
MOCK_PAYMENTS=false
OFFLINE_MODE=false
