# Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Configuration
EXPO_PUBLIC_API_URL=https://api.example.com
EXPO_PUBLIC_API_VERSION=v1

# Authentication
EXPO_PUBLIC_AUTH_URL=https://auth.example.com
EXPO_PUBLIC_CLIENT_ID=your-client-id

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ID=your-analytics-id
EXPO_PUBLIC_ANALYTICS_ENABLED=true

# Feature Flags
EXPO_PUBLIC_FEATURE_FLAGS={"newFeature": true, "betaFeature": false}

# Environment
NODE_ENV=development
EXPO_PUBLIC_ENVIRONMENT=development

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Third-party Services (Optional)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-key
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn

# App Configuration
EXPO_PUBLIC_APP_NAME=Swipe App
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_SUPPORT_EMAIL=support@example.com

# Security
EXPO_PUBLIC_ENCRYPTION_KEY=your-encryption-key
EXPO_PUBLIC_JWT_SECRET=your-jwt-secret

# Development
EXPO_PUBLIC_MOCK_API=false
EXPO_PUBLIC_OFFLINE_MODE=false






