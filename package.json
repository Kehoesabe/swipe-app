{
  "name": "swipe-app",
  "version": "1.0.0",
  "main": "index.js",
  "expo": {
    "web": {
      "entryPoint": "index.web.js"
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "predev": "node scripts/dev-smoke.js",
    "dev:tunnel": "taskkill /IM node.exe /F 2>NUL && npx expo start --tunnel --port 9000",
    "dev:lan": "taskkill /IM node.exe /F 2>NUL && npx expo start --lan --port 9000",
    "dev:clear": "taskkill /IM node.exe /F 2>NUL && npx expo start --clear --tunnel --port 9000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build:web": "expo export -p web",
    "build": "npm run build:web",
    "guardrails:check": "node scripts/guardrails-check.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "1.23.0",
    "@react-native-picker/picker": "^2.7.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-navigation/stack": "^6.4.1",
    "@supabase/supabase-js": "^2.75.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~54.0.12",
    "expo-haptics": "~13.0.0",
    "expo-image-picker": "~17.0.8",
    "expo-status-bar": "~3.0.8",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.17.1",
    "react-native-reanimated": "~3.12.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-web": "^0.21.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.8",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-preset-expo": "^54.0.3",
    "eslint": "^9.36.0",
    "eslint-config-expo": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.9.2"
  },
  "private": true
}